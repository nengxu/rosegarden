<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rosegarden-font-encoding>

<rosegarden-font-encoding name="Feta Scalable">

<font-information
        origin="GNU Lilypond" 
        copyright="Copyright (c) 1997--2001 Han-Wen Nienhuys and Jan Nieuwenhuizen" 
        mapped-by="Chris Cannam"
        type="scalable"
        smooth="true"/>

<font-requirements>
	<font-requirement font-id="0" names="Lilypond-feta,feta,mscore20" strategy="prefer-glyphs"/>
	<font-requirement font-id="1" names="bitstream vera serif,times new roman,times,DEFAULT" strategy="prefer-codes"/> <!-- text font -->
</font-requirements>

<font-sizes>
	<font-scale font-height="4.5" beam-thickness="0.5" staff-line-thickness="0.18" stem-thickness="0.18" border-x="0.08" border-y="0.08"/>
</font-sizes>

<font-symbol-map>
        <!-- Symbol names are from the Unicode code charts: see
             http://www.unicode.org/charts/PDF/U2600.pdf
             http://www.unicode.org/charts/PDF/U1D100.pdf -->

	<codebase font-id="0" base="61440"/> <!-- Unicode private-use area -->

	<symbol name="__UNKNOWN__" font-id="1" code="63"/> <!-- question mark -->

        <symbol name="MUSIC SHARP SIGN" font-id="0" code="63" glyph="17"/>
        <symbol name="MUSIC NATURAL SIGN" font-id="0" code="64" glyph="18"/>
        <symbol name="MUSIC FLAT SIGN" font-id="0" code="65" glyph="19"/>

        <symbol name="MUSICAL SYMBOL DOUBLE SHARP" font-id="0" code="67" glyph="21"/>
        <symbol name="MUSICAL SYMBOL DOUBLE FLAT" font-id="0" code="66" glyph="20"/>

        <symbol name="MUSICAL SYMBOL BREVE" font-id="0" glyph="25"/>
        <symbol name="MUSICAL SYMBOL WHOLE NOTE" font-id="0" code="72" glyph="26"/>
        <symbol name="MUSICAL SYMBOL VOID NOTEHEAD" font-id="0" code="73" glyph="27"/>
        <symbol name="MUSICAL SYMBOL NOTEHEAD BLACK" font-id="0" code="74" glyph="28"/>

	<symbol name="MUSICAL SYMBOL X NOTEHEAD" font-id="0" code="86" glyph="40"/>
	<symbol name="MUSICAL SYMBOL CIRCLE X NOTEHEAD" font-id="0" code="85" glyph="41"/>
<!--	<symbol name="MUSICAL SYMBOL BREVIS" font-id="0" code="71" glyph=""/> -->
<!--	<symbol name="MUSICAL SYMBOL SEMIBREVIS WHITE" font-id="0" code="" glyph=""/> -->
<!--
	<symbol name="MUSICAL SYMBOL SEMIBREVIS BLACK" font-id="0" code="noteheads-2mensural" glyph=""/>
	<symbol name="MUSICAL SYMBOL TRIANGLE NOTEHEAD UP WHITE" font-id="0" code="" glyph=""/>
	<symbol name="MUSICAL SYMBOL TRIANGLE NOTEHEAD UP BLACK" font-id="0" code="" glyph=""/>
-->

        <!-- The inverted glyph for flag-1 in feta has part of a note stem
             attached to it, for some reason: that makes it harder to work
             with correctly, so we'll stick to inverting it ourselves -->
        <symbol name="MUSICAL SYMBOL COMBINING FLAG-1"
                font-id="0" code="133" glyph="97"/>
        <symbol name="MUSICAL SYMBOL COMBINING FLAG-2"
                font-id="0" code="134" inversion-code="140" glyph="98" inversion-glyph="104"/>
        <symbol name="MUSICAL SYMBOL COMBINING FLAG-3" 
                font-id="0" code="135" inversion-code="141" glyph="99" inversion-glyph="105"/>
        <symbol name="MUSICAL SYMBOL COMBINING FLAG-4" 
                font-id="0" code="136" inversion-code="142" glyph="100" inversion-glyph="106"/>

        <symbol name="MUSICAL SYMBOL MULTI REST" font-id="0" glyph="8"/>
        <symbol name="MUSICAL SYMBOL WHOLE REST" font-id="0" code="32" glyph="3"/>
        <symbol name="MUSICAL SYMBOL HALF REST" font-id="0" code="33" glyph="4"/>
        <symbol name="MUSICAL SYMBOL QUARTER REST" font-id="0" code="38" glyph="10"/>
        <symbol name="MUSICAL SYMBOL EIGHTH REST" font-id="0" code="39" glyph="12"/>
        <symbol name="MUSICAL SYMBOL SIXTEENTH REST" font-id="0" code="41" glyph="13"/>
        <symbol name="MUSICAL SYMBOL THIRTY-SECOND REST" font-id="0" code="42" glyph="14"/>
        <symbol name="MUSICAL SYMBOL SIXTY-FOURTH REST" font-id="0" code="43" glyph="15"/>
        <symbol name="MUSICAL SYMBOL ONE HUNDRED TWENTY-EIGHTH REST" font-id="0" code="44" glyph="16"/>

        <symbol name="MUSICAL SYMBOL COMBINING AUGMENTATION DOT" font-id="0" code="70" glyph="24"/>

	<symbol name="MUSICAL SYMBOL COMBINING ACCENT" font-id="0" code="92" glyph="52"/>
	<symbol name="MUSICAL SYMBOL COMBINING TENUTO" font-id="0" code="96" glyph="56"/>
	<symbol name="MUSICAL SYMBOL COMBINING STACCATO" font-id="0" code="93" glyph="53"/>
	<symbol name="MUSICAL SYMBOL COMBINING STACCATISSIMO"
                font-id="0" code="94" inversion-code="95" glyph="54"/>
	<symbol name="MUSICAL SYMBOL COMBINING MARCATO"
                font-id="0" code="99" inversion-code="100" glyph="59"/>
	<symbol name="MUSICAL SYMBOL FERMATA"
	        font-id="0" code="89" inversion-code="90" glyph="43" inversion-glyph="44"/>
	<symbol name="MUSICAL SYMBOL TR" font-id="0" code="107" glyph="67"/>
	<symbol name="MUSICAL SYMBOL TURN" font-id="0" code="106" glyph="66"/>
	<symbol name="MUSICAL SYMBOL COMBINING UP BOW" font-id="0" code="103" glyph="63"/>
	<symbol name="MUSICAL SYMBOL COMBINING DOWN BOW" font-id="0" code="104" glyph="64"/>
        <!-- Unicode (and Lilypond) just have a single G clef, they
             don't distinguish between alto and tenor.  We'll have to
             use the same clef with an offset. -->
        <symbol name="MUSICAL SYMBOL C CLEF" font-id="0" code="145" glyph="109"/>
        <symbol name="MUSICAL SYMBOL G CLEF" font-id="0" code="149" glyph="113"/>
        <symbol name="MUSICAL SYMBOL F CLEF" font-id="0" code="147" glyph="111"/>

	<symbol	name="MUSICAL SYMBOL CUT TIME"  font-id="0" glyph="120"/>
	<symbol	name="MUSICAL SYMBOL COMMON TIME"  font-id="0" glyph="119"/>

<!-- !!! we need to make it possible for the text font to have a different
         size from the notation font! 
	<symbol	name="DIGIT ZERO"  font-id="1" code="48"/>
	<symbol	name="DIGIT ONE"   font-id="1" code="49"/>
	<symbol	name="DIGIT TWO"   font-id="1" code="50"/>
	<symbol	name="DIGIT THREE" font-id="1" code="51"/>
	<symbol	name="DIGIT FOUR"  font-id="1" code="52"/>
	<symbol	name="DIGIT FIVE"  font-id="1" code="53"/>
	<symbol	name="DIGIT SIX"   font-id="1" code="54"/>
	<symbol	name="DIGIT SEVEN" font-id="1" code="55"/>
	<symbol	name="DIGIT EIGHT" font-id="1" code="56"/>
	<symbol	name="DIGIT NINE"  font-id="1" code="57"/>
-->

</font-symbol-map>

