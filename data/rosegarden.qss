    background-color: qlineargradient(spread:pad, x1:0, y1:1, x2:0, y2:0, stop:0 #999999, stop:1 #DDDDDD);
}

#App QLabel {
    background: transparent;
    color: white;
}

#App QToolTip {
    foreground: purple;
    background-color: yellow;
}

