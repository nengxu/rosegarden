        </hotspot>
        <hotspot name="MUSICAL SYMBOL HALF REST">
		<scaled y="0.98"/>
        </hotspot>
        <hotspot name="MUSICAL SYMBOL QUARTER REST">
		<scaled y="0.5"/>
        </hotspot>
        <hotspot name="MUSICAL SYMBOL EIGHTH REST">
		<scaled y="0.4"/>
        </hotspot>
        <hotspot name="MUSICAL SYMBOL SIXTEENTH REST">
